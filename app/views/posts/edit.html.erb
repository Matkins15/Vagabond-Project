<h1>edit post <%= @post.title %></h1>

<%= form_for @post, url: city_post_path(id: @post.id, city_id: @post.city_id) do |f| %>
  <label>Post title:</label><br>
  <%= f.text_field :title %><br>

  <label>Post Description</label><br>
  <%= f.text_area :content %><br>

  <%= f.submit "Update" %>
<% end %>
